[user]
	email = sabonya@sm.rim.or.jp
	name = sabonya
[alias]
	graph = log --graph --date-order --all --pretty=format:'%h %Cred%d %Cgreen%ad %Cblue%cn %Creset%s' --date=short
[core]
	autocrlf = input
	excludesfile = ~/.gitignore
	quotepath = false
[push]
	default = current
[credential]
	helper = store

[url "git@github.com:"]
	pushinsteadof = https://github.com/
